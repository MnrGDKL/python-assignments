    "def FizzBuzzNums():\n",
    "    nums = [\"FizzBuzz\" if i % 15 == 0 \\\n",
    "            else \"Fizz\" if i % 3 == 0 \\\n",
    "            else \"Buzz\" if i % 5 == 0 \\\n",
    "            else i for i in range(1, 101)]\n",
    "    return nums\n",
    "\n",
    "print(*FizzBuzzNums(), sep=\"\\n\") "
   ]
  }

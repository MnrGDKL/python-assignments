   "source": [
    "def primeNumbers(number):\n",
    "    prime_numbers = []\n",
    "    for i in range(2, number+1):\n",
    "        divisers = tuple(j for j in range(1, i + 1) if i % j == 0)\n",
    "        if len(divisers) == 2 :\n",
    "            prime_numbers.append(i)\n",
    "    return prime_numbers\n",
    "\n",
    "print(primeNumbers(int(input(\"n =\"))))"
   ]
  }
